	jQuery(".burger__wrapper").on("click", function () {
		jQuery(".custom-nav__aside").toggleClass("open");
	})

	for (var i = 0; i < comercios.length; i++) {
		if ( jQuery.inArray(comercios[i].poblacion, localidades) < 0 ) {
			localidades.push(comercios[i].poblacion);

		}
	};

	localidades.sort();

	for (var i = 0; i < localidades.length; i++) {
		jQuery("#localidades-map").append('<option value="' + localidades[i] + '">' + localidades[i] + '</option>');
	};


	function add_marker( comercio, map ) {

		// Sacando la url base de la web
		pathArray = location.href.split( '/' );
		protocol = pathArray[0];
		host = pathArray[2];
		iconBase = protocol + '//' + host;

		// Si esta en servidor local
		if (host == "localhost:8888" || host == "localhost" ) {
			iconBase = protocol + '//' + host + '/' + pathArray[3];
		};

		if (pathArray[3] == "payin" ) {
			iconBase = protocol + '//' + host + '/' + pathArray[3];
		};
		
		icon = {
				url: iconBase + '/wp-content/themes/compass-child/assets/marker.svg',
				scaledSize: new google.maps.Size(40, 40)
		};
 
		
		// create marker
		var latlng = new google.maps.LatLng( comercio.longitud, comercio.latitud );

		var marker = new google.maps.Marker({
		  position  : latlng,
		  map     : map,
		  icon: icon,
		  optimized: false,
		});

		marker.id = comercio.id;


		// add to array
		map.markers.push( marker );


		// Create infowindow
		var contentString = '<div id="content">'+
			'<h2 id="firstHeading" class="firstHeading">' + comercio.nombre + '</h2>'+
			(comercio.imagen != "" ? ('<div id="bodyImg"><img src="' + comercio.imagen + '" style="max-width: 120px; margin-bottom: 15px;"></div>') : "") +
			'<div id="bodyContent">'+
			'<p>' + 
			(comercio.direccion != "" ? (comercio.direccion + '<br>') : "") + 
			(comercio.poblacion != "" ? (comercio.poblacion + '<br>') : "") + 
			(comercio.telefono != "" ? (comercio.telefono + '<br>') : "") + 
			(comercio.email != "" ? (comercio.email + '<br>') : "") + 
			(comercio.web != "" ? (comercio.web + '<br>') : "") + 
			'</p>'+
			(comercio.contacto != "" ? ('<p><b>Persona de contacto:</b> ' + comercio.contacto + '</p>') : "") + 
			(comercio.observaciones != "" ? ('<p><b>Observaciones:</b> ' + comercio.observaciones + '</p>') : "") + 
			'</div>'+
			'</div>';

		var infowindow = new google.maps.InfoWindow({
			content: contentString
		});

		infoArray.push(infowindow);


		// Open and close infowindow
		google.maps.event.addListener(marker, 'click', function() {

			for (var i = 0; i < infoArray.length; i++) {
			  infoArray[i].close();
			};

			infoArray[marker.id - 1].open(map, this);

		});
		
	}



	function initialize() {

		//opciones de los mapas. 
		var mapOptions = {
			  zoom: 10,
			  // mapTypeControl: false,
			  zoomControl: true,
			  // streetViewControl: false, 
			  scrollwheel: true,
			  draggable: true,
		}

		// ejecución del los dos mapas
		map = new google.maps.Map(document.getElementById('map-comercios'), mapOptions);


		//markers
		map.markers = [];

		//loop para añadir markers de los puntos de interes
		for (var i = 0; i < comercios.length; i++) {
			add_marker( comercios[i], map );
		};


		//Centrar map para que se vean todos los puntos
		bounds = new google.maps.LatLngBounds();
		for(i=0;i<map.markers.length;i++) {
			  bounds.extend(map.markers[i].getPosition());
		}
		map.fitBounds(bounds);

	}

	jQuery(document).ready(function() {
	
		// Init
		google.maps.event.addDomListener(window, 'load', initialize);


		jQuery("#provincias-map").on("change", function () {

			var provincia = "provincia de " + jQuery( "#provincias-map option:selected" ).attr("value");

			if ( provincia == "provincia de all") {

				for(i=0;i<map.markers.length;i++) {
					  bounds.extend(map.markers[i].getPosition());
				}
				map.fitBounds(bounds);

			} else {

				geocoder.geocode( {'address' : provincia}, function(results, status) {
					if (status == google.maps.GeocoderStatus.OK) {
						console.log(results);
						map.setCenter(results[0].geometry.location);
        				map.fitBounds(results[0].geometry.viewport);
					}
				});

				// map.setZoom(10);
			}

				
		})



		jQuery("#localidades-map").on("change", function () {

			var localidad = jQuery( "#localidades-map option:selected" ).attr("value");

			if ( localidad == "all") {

				for(i=0;i<map.markers.length;i++) {
					  bounds.extend(map.markers[i].getPosition());
				}
				map.fitBounds(bounds);

			} else {

				geocoder.geocode( {'address' : localidad}, function(results, status) {
					if (status == google.maps.GeocoderStatus.OK) {
						console.log(results);
						map.setCenter(results[0].geometry.location);
        				map.fitBounds(results[0].geometry.viewport);
					}
				});

				// map.setZoom(10);
			}

				
		})

	})
</script>